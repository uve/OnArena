{{ jsoncallback }}({
		"title": "Cometip.com Statistics",
		"link": "http://www.cometip.com/",
		"description": "",
		"modified": "2011-03-30T06:43:12Z",
		"generator": "http://www.cometip.com/",
		"items": [
  {% for player in all_goals %}
   {
  "name": '<a class="sw-link" href="{{ DOMAIN }}/player/{{ player.player_id.id }}/?team_id={{ player.team_id.id }}">{% if player.player_id.second_name %}{{ player.player_id.second_name }}{% endif%} {% if player.player_id.name %}{{ player.player_id.name }}{% endif %}</a>',
  "team": '<a class="sw-link" href="{{ DOMAIN }}/team/{{ player.team_id.id }}/">{{ player.team_id.name }}</a>',
  "goals": {{ player.score }},
  "cards": '{% if player.yellow_cards %}<img src="http://commondatastorage.googleapis.com/cometip/images/yellow_card.png" height="18"> {{ player.yellow_cards }}    {% endif %}{% if player.red_cards %}<img src="http://commondatastorage.googleapis.com/cometip/images/red_card.png" height="18"> {{ player.red_cards }}{% endif %}'
   }   
    {% if not forloop.last %},{% endif %}
  {% endfor %}
  ]
})
